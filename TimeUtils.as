package  {		public class TimeUtils {//		public function TimeUtils() {//			// constructor code//		}		public static function makeTime(n:Number):String{			n < 0 ? n = 0: null			n.toString() == "NaN"? n = 0 : null			var secs = String(Math.floor(n)%60)			secs.length < 2 ? secs = "0" + secs : null;			var mins = String(Math.floor(n/60)%60)			mins.length < 2 ? mins = "0" + mins : null;			var hors = String(Math.floor(n/60/60)%60)			hors.length < 2 ? hors = "0" + hors : null;				return  hors + ":" + mins + ":" + secs		}		public static function makeTimeIntoSeconds(n:String):Number{			var t:Array = n.split(":")			var a = (Number(t[0]) * 3600) + (Number(t[1]) * 60) + (Number(t[2]))			return a		}	}	}